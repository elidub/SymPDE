#!/bin/bash

sympde_dir=$(pwd)/sympde
cd "$sympde_dir"

source activate sympde

pde_name="KdV"
max_epochs=10

for seed in {1..3}
do
    python run.py --train --pde_name $pde_name --version ${pde_name}_seed$seed \
        --max_epochs $max_epochs --seed $seed \
        --local
done

